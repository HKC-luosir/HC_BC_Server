<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connections>
    <PLCConnection>
      <PLC>
        <!--公用配置-->
        <Name>Loader</Name>
        <!--Label-->
        <PLCType>MESEL</PLCType>
        <LogLevel>DEBUG</LogLevel>
        <LogPath>logs/PLCLog</LogPath>
        <ChannelNo>00</ChannelNo>
        <TransmissonMode>ASCII</TransmissonMode>
        <!--ASCII/Binary. Modbus的RTU形式就是Binary，请配置成Binary-->
        <MelsecFrame>3E</MelsecFrame>
        <!--1C,2C,3C,4C,3E,4E.其中1C-4C是串口通信协议相关，3E,4E为网络通信协议相关-->
        <ErrorCheckingMethod>CRC</ErrorCheckingMethod>
        <!--Parity/LRC/CRC-->
        <Timeout>5000</Timeout>
        <ConnectInterval>5</ConnectInterval>
        <WzoneScanIntervalInMsec>40</WzoneScanIntervalInMsec>
        <BzoneScanIntervalInMsec>10</BzoneScanIntervalInMsec>
        <PLCMapFile>Configuration\EIP\OLB\LabelAddressMap.xml</PLCMapFile>
        <WorkflowFile>Configuration\EIP\OLB\LabelWorkflow.xml</WorkflowFile>
        <EquipmentList></EquipmentList>
        <Protocol>TCP</Protocol>
        <!--支持串口：COM  TCP-->
        <!--串口连接配置-->
        <COMName>COM3</COMName>
        <!--串口的名称-->
        <BaudRate>9600</BaudRate>
        <!--串口波特率-->
        <Parity>Odd</Parity>
        <!--None,Odd,Even,Mark,Space-->
        <SumCheck>NO</SumCheck>
        <!--YES-->
        <MelsecFormat>3</MelsecFormat>
        <!--只有串口通信时需要配置。可配选项：1,2,3,4,5. 其中1，2，3，4是关联ASCII的格式，5是Binary-->
        <!--网络连接配置-->
        <RemoteIP>192.168.0.102</RemoteIP>
        <RemotePort>7001</RemotePort>
        <ConnectMode>ACTIVE</ConnectMode>
      </PLC>
    </PLCConnection>
	  <HSMSConnection>
		  <Name>TEST01</Name>
		  <DeviceID>1</DeviceID>
		  <ConnectionMode>ACTIVE</ConnectionMode>
		  <LocalIP>127.0.0.1</LocalIP>
		  <LocalPort>7000</LocalPort>
		  <RemoteIP>127.0.0.1</RemoteIP>
		  <RemotePort>7000</RemotePort>
		  <LogLevel>DEBUG</LogLevel>
		  <LogPath>Logs\SECSLog\TEST01</LogPath>
		  <SECSLibraryFile>Configuration\SECS\SECSLibrary.xml</SECSLibraryFile>
		  <EQPType>eView100</EQPType>
		  <AsciiValueAutoFillSpace>true</AsciiValueAutoFillSpace>
		  <T1>10</T1>
		  <T2>10</T2>
		  <T3>10</T3>
		  <T4>10</T4>
		  <T5>10</T5>
		  <T6>10</T6>
		  <T7>10</T7>
		  <T8>10</T8>
	  </HSMSConnection>
  </connections>
  <AppConfig UseAppDomain="1"/>
</configuration>